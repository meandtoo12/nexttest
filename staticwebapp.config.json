{
  "routes": [
      {
          "route": "/profile*",
          "allowedRoles": ["authenticated"]
      },
      {
          "route": "/api/auth/logout",
          "redirect": "/api/auth/login",
          "allowedRoles": [
            "anonymous",
            "authenticated"
          ]
      },
      {
          "route": "/api/auth/login",
          "allowedRoles": ["anonymous"]
      }
  ]
}
